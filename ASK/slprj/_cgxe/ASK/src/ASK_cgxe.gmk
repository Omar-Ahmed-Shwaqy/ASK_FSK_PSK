USER_INCLUDES = -I"D:\الجامعه\ترم خامس اتصالات\communication(2)\Task\ASK\slprj\_cgxe\ASK\src" -I"D:\الجامعه\ترم خامس اتصالات\communication(2)\Task\ASK" -I"D:\الجامعه\ترم خامس اتصالات\Communication(2)\Task\ASK\slprj\_cprj" 
